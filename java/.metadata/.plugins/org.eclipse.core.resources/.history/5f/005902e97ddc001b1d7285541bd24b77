package read_csv;

public final class Constants {
	// DB CONSTANTS 
	public static final String DB_URL = "jdbc:mysql://localhost:3306/csv_database";
	public static final String DB_NAME = "";
	public static final String DB_USER = "csv_user";
	public static final String DB_PASSWORD = "csv_password";
	
	// THREAD CONSTANTS
	public static final String SAVE_COUNT = "SAVE_COUNT";	// key in thread return map, containing count of success save count
	public static final Integer BATCH_LIMIT = 50;
	public static final Integer QUEUE_SIZE = 65536;
	
	public static final String MAKE_TABLE_SQL = "CREATE TABLE table_name (\n" + 
			"    column1 datatype,\n" + 
			"    column2 datatype,\n" + 
			"    column3 datatype,\n" + 
			"   ....\n" + 
			"); ";
	
	public static final String DATETIME_REGEX = "(\\d{4})-(\\d{2})-(\\d{2}) (\\d{2}):(\\d{2}):(\\d{2})";
}
